    "express": "^4.17.1",
    "mapmyindia-map-react-native-beta": "^0.0.2",
    "mapmyindia-react": "^1.0.4",
    "postcss-cli": "^6.1.3",
    "razzle": "^3.0.0",
    "axios": "^0.18.0",
    "react": "^16.12.0",
    "pg": "^7.8.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "tailwindcss": "^1.1.4"
  }
}
